from sklearn.ensemble import XGboost
import joblib
import pandas as pd

df = pd.read_csv("data/transactions.csv")
features = df[["amount", "tx_count_24h", "is_blacklisted"]]  # adjust as needed

model = XGboost()
model.fit(features)

joblib.dump(model, "models/xgboost.pkl")
